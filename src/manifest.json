{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "name": "Modern JSON Formatter",
  "description": "Format JSON in a modern way. With guaranteed order of keys, big numbers, expandable/collapsable properties and more.",
  "version": "0.0.0",
  "manifest_version": 3,
  "homepage_url": "https://github.com/evg4b/modern-json-formatter",
  "key": "",
  "short_name": "JSON fmt",
  "minimum_chrome_version": "88",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "worker-core.wasm",
        "invalid.svg",
        "faq.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "*.map"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  }
}
