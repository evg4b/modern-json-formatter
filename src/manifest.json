{
  "name": "Modern JSON Formatter",
  "description": "Format JSON in a modern way. With guaranteed order of keys, big numbers, expandable/collapsable properties and more.",
  "version": "0.0.0",
  "manifest_version": 3,
  "homepage_url": "https://github.com/evg4b/modern-json-formatter",
  "key": "dmofgolehdakghahlgibeaodbahpfkpf",
  "short_name": "JSON fmt",
  "minimum_chrome_version": "88",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "parser.wasm",
        "invalid.svg"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  }
}
