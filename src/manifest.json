{
  "name": "Modern JSON Formatter",
  "version": "0.0.1",
  "manifest_version": 3,
  "homepage_url": "https://github.com/evg4b/json-formatter",
  "minimum_chrome_version": "88",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "parser.wasm"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  }
}
