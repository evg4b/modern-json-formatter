{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "esModuleInterop": true,
    "allowJs": false,
    "types": [
      "node",
      "chrome",
      "golang-wasm-exec"
    ],
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@tokenizer": [
        "packages/tokenizer"
      ],
      "@jq": [
        "packages/jq"
      ]
    }
  }
}
